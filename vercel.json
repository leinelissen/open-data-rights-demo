{
    "rewrites": [
        { "source": "/oauth/authorize", "destination": "/api/oauth/authorize" },
        { "source": "/oauth/token", "destination": "/api/oauth/token" },
        { "source": "/oauth/callback", "destination": "/static/index.html" },
        { "source": "/data", "destination": "/api/data" },
        { "source": "/data/me", "destination": "/api/data/me" },
        { "source": "/requests", "destination": "/api/requests" },
        { "source": "/requests/:requestId", "destination": "/api/requests/[requestId]" },
        { "source": "/requests/:requestId/complete", "destination": "/api/requests/[requestId]/complete" },
        { "source": "/requests/:requestId/download", "destination": "/api/requests/[requestId]/download" },
        { "source": "/settings", "destination": "/settings" }
    ]
}